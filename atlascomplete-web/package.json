{
    "name": "atlascomplete-web",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "concurrently \"npm run serve-api\" \"npm run serve\"",
        "serve": "ng serve --port 4200 --proxy-config proxy.config.json",
        "serve-api": "nodemon server/webserver.js --watch server",
        "build": "ng build",
        "test": "ng test",
        "lint": "tslint --project \".\"",
        "lint:fix": "tslint --project \".\" --fix",
        "lint:styles": "stylelint \"src/**/*.scss\"",
        "e2e": "ng e2e",
        "format": "prettier --write \"src/**/*.{html,js,md,ts,graphql,yaml,yml,json,vue}\" && prettier-stylelint --write \"src/**/*.{css,scss}\"",
        "compodoc": "compodoc -p src/tsconfig.app.json -s -w"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "src/**/*.{html,js,md,ts,graphql,yaml,yml,json,vue}": [
            "prettier --write",
            "git add"
        ],
        "src/**/*.{css,scss}": [
            "prettier-stylelint --write",
            "git add"
        ]
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^7.0.2",
        "@angular/cdk": "^7.0.2",
        "@angular/common": "^7.0.2",
        "@angular/compiler": "^7.0.2",
        "@angular/core": "^7.0.2",
        "@angular/elements": "^7.1.3",
        "@angular/forms": "^7.0.2",
        "@angular/http": "^7.0.2",
        "@angular/material": "^7.0.2",
        "@angular/platform-browser": "^7.0.2",
        "@angular/platform-browser-dynamic": "^7.0.2",
        "@angular/router": "^7.0.2",
        "@webcomponents/custom-elements": "^1.2.1",
        "atlas-web-components": "file:../atlas-web-component/dist/atlas-web-components/atlas-web-components-0.0.1.tgz",
        "atlas-web-services": "file:../atlas-services/dist/atlas-web-services/atlas-web-services-0.0.1.tgz",
        "bootstrap": "^3.3.7",
        "core-js": "^2.5.4",
        "document-register-element": "^1.13.1",
        "font-awesome": "^4.7.0",
        "hammerjs": "^2.0.8",
        "ng2-pdf-viewer": "^5.2.3",
        "rxjs": "~6.3.3",
        "rxjs-compat": "^6.3.3",
        "zone.js": "~0.8.26"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^0.11.4",
        "@angular-devkit/build-ng-packagr": "~0.10.0",
        "@angular/cli": "~7.0.4",
        "@angular/compiler-cli": "^7.0.2",
        "@angular/language-service": "^7.0.2",
        "@compodoc/compodoc": "^1.1.7",
        "@types/jasmine": "~2.8.8",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "~10.12.2",
        "body-parser": "^1.18.3",
        "codelyzer": "~4.5.0",
        "concurrently": "^4.0.1",
        "express": "^4.16.4",
        "husky": "^1.3.1",
        "jasmine-core": "~3.3.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~3.1.1",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "~2.0.1",
        "karma-jasmine": "~1.1.2",
        "karma-jasmine-html-reporter": "^1.4.0",
        "lint-staged": "^8.1.0",
        "ng-packagr": "^4.2.0",
        "nodemon": "^1.18.6",
        "prettier": "^1.15.3",
        "prettier-stylelint": "^0.4.2",
        "protractor": "~5.4.0",
        "stylelint": "^9.9.0",
        "stylelint-config-standard": "^18.2.0",
        "stylelint-scss": "^3.4.4",
        "ts-node": "~7.0.0",
        "tsickle": ">=0.29.0",
        "tslib": "^1.9.0",
        "tslint": "~5.11.0",
        "tslint-config-prettier": "^1.17.0",
        "tslint-lines-between-class-members": "^1.3.1",
        "typescript": "~3.1.6"
    }
}
